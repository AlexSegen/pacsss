{
    "presets": ["env"],
    "plugins": [
        "lodash",
        "syntax-flow",
        "transform-flow-strip-types",
        "transform-proto-to-assign",
        "transform-class-properties",
        ["transform-es2015-classes", {
            "loose": true
        }],
      ["transform-runtime",
      {
          "polyfill": false,
          "regenerator": true
      }]
  ]
  }